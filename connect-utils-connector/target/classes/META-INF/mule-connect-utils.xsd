    </xs:annotation>
  </xs:element>
  <xs:complexType name="Get-date-as-x-m-l-gregorian-calendarType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/connect-utils" type="dateTimeType" use="required" name="date">
          <xs:annotation>
            <xs:documentation>the date</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
