      <xs:documentation>This method formats the EDIFACT date</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Get-formatted-dateType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="inputDTM">
          <xs:annotation>
            <xs:documentation>the date to b formated</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/connect-utils" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Get-date-as-x-m-l-gregorian-calendarType" substitutionGroup="mule:abstract-message-processor" name="get-date-as-x-m-l-gregorian-calendar">
